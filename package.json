{
  "name": "pok-mon",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development supervisor --harmony ./bin/www",
    "local": "cross-env NODE_ENV=local supervisor --harmony ./bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production pm2 start ./bin/www --node-args='--harmony' --name 'node-element'",
    "stop": "cross-env NODE_ENV=production pm2 stop ./bin/www --name 'node-element'",
    "restart": "cross-env NODE_ENV=production pm2 restart ./bin/www --node-args='--harmony' --name 'node-element'"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "config-lite": "^2.1.0",
    "connect-history-api-fallback": "^1.5.0",
    "connect-mongo": "^2.0.1",
    "connect-mysql": "^2.1.7",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "express-winston": "^3.0.0",
    "formidable": "^1.2.1",
    "iconv-lite": "^0.4.23",
    "jade": "~1.11.0",
    "log4js": "^3.0.5",
    "mongoose": "^5.2.9",
    "mysql": "^2.16.0",
    "node-fetch": "^2.2.0",
    "pm2": "^3.0.4",
    "qiniu": "^7.2.1",
    "request": "^2.87.0",
    "supervisor": "^0.12.0",
    "time-formater": "^1.0.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0"
  }
}
